	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9b9c9b",
			}
		},
		{
			"scope": "markup.underline.link",
			"settings": {
				"name": "Link",
				"foreground": "#7bd88f"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.template"
			],
			"settings": {
				"foreground": "#b4a800"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#be60db"
			}
		},
		{
			"name": "Embedded String Begin/End",
