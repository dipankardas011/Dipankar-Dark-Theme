	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7b787e"
			}
		},
		{
            "scope": "markup.underline.link",
            "settings": {
                "name": "Link",
                "foreground": "#7bd88f"
            }
        },
		{
            "scope": [
                "meta.brace.round",
                "meta.brace.square",
                "meta.brace.curly",
                "meta.delimiter.comma.js",
                "meta.function-call.without-arguments.js",
                "meta.function-call.method.without-arguments.js"
            ],
            "settings": {
                "name": "Braces, delimiters",
                "foreground": "#7b787e"
            }
        },
		{
            "scope": "punctuation",
            "settings": {
                "name": "Punctuation",
                "foreground": "#7b787e"
            }
        },
		{
			"name": "String",
			"scope": [
				"string",
				"string.template"
			],
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"name": "Embedded String Begin/End",
